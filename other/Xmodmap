! Swap caps lock and escape
! remove Lock = Caps_Lock
! keysym Escape = Caps_Lock
! keysym Caps_Lock = Escape
! add Lock = Caps_Lock

! Only map CapsLock to Esc
! remove Lock = Caps_Lock
! keysym Caps_Lock = Escape

clear lock
clear control
add control = Caps_Lock Control_L Control_R
keycode 66 = Control_L Caps_Lock
